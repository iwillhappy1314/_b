{
  "name": "wenprise/_b",
  "description": "Plugin developed by wenprise",
  "require": {
    "php": ">=5.6",
    "ext-json": "*",
    "wenprise/wp-db-base": "^1.0",
    "wenprise/wordpress-dispatcher": "^2.1",
    "wenprise/eloquent": "^0.1.2"
  },
  "require-dev": {
    "wenprise/debug": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "WenpriseSpaceName\\": "src/"
    },
    "classmap": [
      "build/"
    ]
  },
  "license": "GPL",
  "authors": [
    {
      "name": "Amos Lee",
      "email": "iwillhappy1314@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "scripts": {
    "pre-install-cmd": [
      "rm -rf build",
      "mkdir -p build"
    ],
    "pre-update-cmd": [
      "rm -rf build",
      "mkdir -p build"
    ],
    "post-install-cmd": [
      "php-scoper add-prefix",
      "grep \"'Nette\\\\\\\\\" -rl build | xargs sed -i '' \"s/'Nette\\\\\\\\/'WenpriseAffiliateVendor\\\\\\\\\\\\\\Nette\\\\\\\\/g\"",
      "grep \"'Wenprise\\\\\\\\\" -rl build | xargs sed -i '' \"s/'Wenprise\\\\\\\\/'WenpriseAffiliateVendor\\\\\\\\\\\\\\Wenprise\\\\\\\\/g\"",
      "grep \"@return \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@return \\\\\\Illuminate/'@return \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@var \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@var \\\\\\Illuminate/'@var \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@param \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@param \\\\\\Illuminate/'@param \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@param  \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@param  \\\\\\Illuminate/'@param \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@mixin \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@mixin \\\\\\Illuminate/'@mixin \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@throws \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@throws \\\\\\Illuminate/'@throws \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"|\\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/|\\\\\\Illuminate/'|\\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "rm -rf vendor/*",
      "composer dump-autoload"
    ],
    "post-update-cmd": [
      "php-scoper add-prefix",
      "grep \"'Nette\\\\\\\\\" -rl build | xargs sed -i '' \"s/'Nette\\\\\\\\/'WenpriseAffiliateVendor\\\\\\\\\\\\\\Nette\\\\\\\\/g\"",
      "grep \"'Wenprise\\\\\\\\\" -rl build | xargs sed -i '' \"s/'Wenprise\\\\\\\\/'WenpriseAffiliateVendor\\\\\\\\\\\\\\Wenprise\\\\\\\\/g\"",
      "grep \"@return \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@return \\\\\\Illuminate/'@return \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@var \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@return \\\\\\Illuminate/'@return \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@param \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@param \\\\\\Illuminate/'@param \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@param  \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@param  \\\\\\Illuminate/'@param \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@mixin \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@mixin \\\\\\Illuminate/'@mixin \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"@throws \\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/@throws \\\\\\Illuminate/'@throws \\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "grep \"|\\\\\\Illuminate\" -rl build | xargs sed -i '' \"s/|\\\\\\Illuminate/'|\\\\\\WenpriseAffiliateVendor\\\\\\Illuminate/g\"",
      "rm -rf vendor/*",
      "composer dump-autoload"
    ]
  }
}
